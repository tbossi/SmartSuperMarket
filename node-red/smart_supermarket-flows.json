  {
    "id": "b445821a.a2f2b8",
    "type": "ui_template",
    "z": "186b7a65.42ec46",
    "group": "9d4a3a62.d7ce4",
    "name": "Carts Heatmap",
    "order": 0,
    "width": "10",
    "height": "12",
    "format": "<style>\n    #grid table {\n        border-collapse: collapse;\n        border-spacing: 0;\n    }\n    #grid .cell {\n        color: black;\n        width: 45px;\n        height: 45px;\n        text-align: center;\n        line-height: 40px;\n        padding: 2px;\n        box-sizing: border-box;\n        border: 1px dashed grey;\n        background: rgb(245,245,245);\n        margin: 0;\n    }\n    #grid .cell.alone {\n        background: rgb(10,245,10);\n    }\n    #grid .cell.half {\n        background: rgb(245,245,10);\n    }\n    #grid .cell.full {\n        background: rgb(245,10,10);\n    }\n</style>\n<div id=\"grid\">\n    <table>\n        <tr ng-repeat=\"row in msg.grid.grid track by $index\">\n            <td ng-repeat=\"cell in row track by $index\">\n                <div class=\"cell\" ng-class=\"{\n                    alone: cell==1,\n                    half: cell>1 && cell<4,\n                    full: cell>=4\n                    }\">{{cell}}</div>\n            </td>\n        </tr>\n    </table>\n</div>",
