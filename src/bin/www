#!/usr/bin/env node
'use strict';
const httpServer = require('../servers/http-server');
const tcpServer = require('../servers/tcp-server');

class serverContainer {
    constructor(...servers) {
        this.servers = servers;
    }

    start() {
        this.servers.forEach(s => s.start());
    }
}

new serverContainer(
    new httpServer(process.env.PORT || '3000'),
    new tcpServer(9090)
).start();